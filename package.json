    "babel-preset-es2015": "6.13.2",
    "coveralls": "2.11.12",
    "cz-conventional-changelog": "1.2.0",
    "eslint": "3.3.1",
    "eslint-config-google": "0.6.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-jasmine": "1.8.1",
    "html-loader": "0.4.3",
    "isparta-loader": "2.0.0",
    "jasmine": "2.5.2",
    "jasmine-core": "2.4.1",
    "karma": "1.2.0",
    "karma-coverage": "1.1.1",
