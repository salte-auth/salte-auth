{
  "name": "salte-auth",
  "version": "0.0.0-semantically-released.0",
  "main": "dist/salte-auth.js",
  "description": "Test",
  "homepage": "https://github.com/salte-io/salte-auth",
  "bugs": "https://github.com/salte-io/salte-auth/issues",
  "contributors": [
    "Nick Woodward <nick@salte.io>"
  ],
  "keywords": [
    "salte-auth"
  ],
  "engines": {
    "node": ">= 5.4.1 < 6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/salte-io/salte-auth.git"
  },
  "scripts": {
    "lint": "eslint src/salte-auth.js tests/salte-auth/salte-auth.spec.js",
    "test": "zuul -- tests/**/*.spec.js",
    "test:local": "zuul --local 8080 -- tests/**/*.spec.js",
    "test:phantom": "zuul --phantom -- tests/**/*.spec.js",
    "build": "webpack --progress --colors -d",
    "build:prod": "webpack --progress --colors -p",
    "tdd": "watch \"npm run test:phantom\" src tests",
    "check-coverage": "istanbul check-coverage --statements 93 --branches 89 --functions 92 --lines 92",
    "report-coverage": "coveralls < coverage/lcov.info",
    "publish-latest": "publish-latest --user-email nick+salte-bot@salte.io --user-name salte-bot",
    "semantic-release": "semantic-release pre && npm run build:prod && npm publish && npm run publish-latest && semantic-release post"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.13.2",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.13.2",
    "chai": "3.5.0",
    "coveralls": "2.11.12",
    "cz-conventional-changelog": "1.2.0",
    "eslint": "3.3.1",
    "eslint-config-google": "0.6.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-mocha": "4.7.0",
    "html-loader": "0.4.3",
    "isparta-loader": "2.0.0",
    "phantomjs-prebuilt": "2.1.13",
    "publish-latest": "1.1.2",
    "semantic-release": "4.3.5",
    "sinon": "1.17.6",
    "watch": "1.0.1",
    "webpack": "1.13.1",
    "zuul": "3.11.1",
    "zuul-builder-webpack": "nick-woodward/zuul-builder-webpack"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
